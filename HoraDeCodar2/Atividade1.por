programa {
  funcao inicio() {
    real numeroUm
    real numeroDois

    escreva("Escreva o primeiro número: ")
    leia (numeroUm)

    escreva("Escreva o segundo número: ")
    leia (numeroDois)

    se (numeroUm > numeroDois){
      escreva("o número maior é:", numeroUm)
    }
    senao (escreva("o número maior é:", numeroDois))
  }
}